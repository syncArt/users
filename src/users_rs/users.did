type SocialMedia = record { nickname : text; name : text };
type User = record {
  name : text;
  description : text;
  social_media : vec SocialMedia;
};
service : {
  get : (principal, text) -> (User) query;
  getSelf : (principal, ) -> (User) query;
  search : (principal, text) -> (opt User) query;
  update : (principal, User) -> ();
}
