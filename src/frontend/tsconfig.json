{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "composite": true,
    "declaration": true,
    "outDir": "../../HACK_BECAUSE_OF_ALLOW_JS/smileyball_frontend",
    "rootDir": ".",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": false,
    "jsx": "react-jsx",
    "typeRoots": [
      "../../node_modules/@types",
      "../../node_modules"
    ],
    "types": [
      "vite/client"
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "declarations/*": [
        "../declarations/*"
      ]
    }
  },
  "include": [
    "vite.config.ts",
    "src/**/*",
    "tests/**/*"
  ],
  "exclude": [
    "dist",
    "node_modules",
    "../../node_modules"
  ]
}
